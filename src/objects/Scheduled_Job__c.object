    <validationRules>
        <fullName>sj_Job_Required_Week_Days</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
ISPICKVAL(Frequency__c, &apos;Weekly&apos;),
ISBLANK(Week_Days__c) 
)</errorConditionFormula>
        <errorMessage>Minimum 1 day needs to be selected when the frequency has been set to weekly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sj_Required_Class</fullName>
        <active>true</active>
        <errorConditionFormula>ApexClass__c = &apos;None&apos;</errorConditionFormula>
        <errorMessage>A batch or schedule class needs to be selected for the job.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sj_Required_Frequency</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Frequency__c, &apos;&apos;)</errorConditionFormula>
        <errorMessage>A frequency needs to be selected for the job.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sj_Required_Preferred_Time</fullName>
        <active>true</active>
        <errorConditionFormula>OR
(
 ISPICKVAL(Preferred_Start_Hour__c, &apos;&apos;),
 ISPICKVAL(Preferred_Start_Minutes__c, &apos;&apos;)
)</errorConditionFormula>
        <errorMessage>The hour and minutes of the preferred start time are required.</errorMessage>
    </validationRules>
